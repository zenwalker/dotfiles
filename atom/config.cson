"*":
  Zen:
    fullscreen: false
    softWrap: true
    width: 70
  "autocomplete-plus":
    autoActivationDelay: 200
  "autocomplete-python":
    showDescriptions: false
  core:
    audioBeep: false
    disabledPackages: [
      "minimap-autohide"
      "dev-live-reload"
      "language-ruby-on-rails"
      "language-objective-c"
      "language-mustache"
      "language-csharp"
      "language-clojure"
      "language-c"
      "welcome"
      "metrics"
      "wrap-guide"
      "language-python"
      "minimap"
      "spell-check"
      "background-tips"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".cache"
      "*.orig"
      "*.pyc"
      "*.egg-info"
      "__pycache__"
      "node_modules"
      "bower_components"
      ".DS_Store"
      ".venv"
    ]
    openEmptyEditorOnStart: false
  editor:
    fontFamily: "Ubuntu Mono"
    fontSize: 23
    invisibles: {}
    lineHeight: 1.3
    scrollPastEnd: true
    showInvisibles: true
    tabType: "soft"
  emmet:
    extensionsPath: "~/.dotfiles/emmet"
    stylus:
      autoInsertVendorPrefixes: false
      valueSeparator: ": "
  "exception-reporting":
    userId: "f97c5b89-0f81-089d-a5ba-144a2164b68d"
  "file-icons":
    coloured: false
    tabPaneIcon: false
  "fuzzy-finder": {}
  "highlight-selected": {}
  "isotope-light-ui": {}
  "isotope-ui":
    minimalMode: true
  jekyll: {}
  "language-babel":
    babelMapsAddUrl: false
    transpileOnSave: false
  linter:
    lintOnFlyInterval: 1000
    showErrorPanel: false
    showErrorTabFile: false
    showErrorTabProject: false
    showProviderName: false
  "linter-eslint": {}
  "linter-flake8":
    executablePath: "/Users/maxim/.virtualenvs/py35/bin/flake8"
  "markdown-preview":
    useGitHubStyle: true
  "markdown-writer":
    siteEngine: "jekyll"
  minimap:
    plugins:
      "minimap-autohide": true
  "one-dark-ui":
    layoutMode: "Compact"
  "one-light-ui": {}
  pigments: {}
  "python-indent": {}
  react: {}
  "spell-check": {}
  "status-bar": {}
  tabs: {}
  tasks: {}
  "tree-view":
    hideIgnoredNames: true
  "unity-ui": {}
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".basic.html.text":
  editor:
    softWrap: true
".gfm.source":
  editor:
    showInvisibles: false
    softWrapAtPreferredLineLength: true
".go.source":
  editor:
    tabLength: 4
".html.php.text":
  editor:
    tabLength: 4
".plain.text":
  editor:
    softWrap: true
".restructuredtext.text source.gfm.restructuredtext":
  editor:
    tabLength: 4
".shell.source":
  editor:
    tabLength: 4
